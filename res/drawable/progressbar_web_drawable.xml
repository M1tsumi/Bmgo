<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="@color/progressBar_nor" android:endColor="@color/progressBar_nor" android:angle="270.0" android:centerY="2.0" android:centerColor="@color/progressBar_nor" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/progressBar_pre" android:endColor="@color/progressBar_pre" android:angle="270.0" android:centerY="2.0" android:centerColor="@color/progressBar_pre" />
            </shape>
        </clip>
    </item>
</layer-list>
