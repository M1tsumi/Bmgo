<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff444444" android:endColor="#ff444444" android:angle="270.0" android:centerY="2.0" android:centerColor="#ff444444" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="@color/btnNorNew" android:endColor="@color/btnNorNew" android:angle="270.0" android:centerY="2.0" android:centerColor="@color/btnNorNew" />
            </shape>
        </clip>
    </item>
</layer-list>
