<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="@color/white" android:endColor="@color/white" android:angle="270.0" android:centerY="0.0" android:centerColor="@color/white" />
            <stroke android:width="1.5dip" android:color="@color/vip_page_bg" android:dashWidth="1.0dip" android:dashGap="0.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="@color/switch_version_progress_bar_color" android:endColor="@color/switch_version_progress_bar_color" android:angle="270.0" android:centerY="0.0" android:centerColor="@color/switch_version_progress_bar_color" />
                <stroke android:width="1.5dip" android:color="@color/vip_page_bg" android:dashWidth="1.0dip" android:dashGap="0.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>
