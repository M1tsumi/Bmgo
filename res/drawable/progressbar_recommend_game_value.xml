<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="6.0dip" />
            <gradient android:startColor="#ff22ac38" android:endColor="#ff22ac38" android:angle="270.0" android:centerY="0.0" android:centerColor="#ff22ac38" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="6.0dip" />
                <gradient android:startColor="#ff5dcb13" android:endColor="#ff5dcb13" android:angle="270.0" android:centerY="0.0" android:centerColor="#ff5dcb13" />
            </shape>
        </clip>
    </item>
</layer-list>
